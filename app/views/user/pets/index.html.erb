<h1>ペット一覧</h1>
<thead>
    <tr>
      <th>写真</th>
      <th>タイトル</th>
    </tr>
  </thead>
  <tbody>
    <% @pets.each do |pet| %>
      <tr>
        <td>
          <%= image_tag pet.get_image, width: 150, height: 150 %>
          </td>
        <td>
          <%= link_to pet_path(pet.id) do %>
            <%= pet.name %>
          <% end %>
          </td>
        <td><%#= pet.body %></td>
      </tr>
    <% end %>
    <%= link_to "ペット登録", new_pet_path %>
  </tbody>

